<?xml version="1.0" encoding="utf-8"?>
<!-- 定義一個 InsetDrawable，設置內邊距 -->
<inset xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- 定義形狀 -->
    <shape>
        <!-- 設置背景顏色為淡紫色 -->
        <solid android:color="#D1E9E9"/>
        <!-- 設置邊角圓度為 8dp -->
        <corners android:radius="8dp"/>
    </shape>
</inset>
